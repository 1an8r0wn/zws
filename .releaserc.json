{
	"branches": ["main"],
	"plugins": [
		[
			"@semantic-release/commit-analyzer",
			{
				"preset": "angular"
			}
		],
		"@semantic-release/release-notes-generator",
		"@semantic-release/github",
		[
			"@semantic-release/exec",
			{
				"prepareCmd": "bash ./scripts/prepare.bash ${nextRelease.version}",
				"publishCmd": "bash ./scripts/publish.bash ${nextRelease.version}"
			}
		]
	]
}
